Code for Block Scope
------------------------


#include <iostream>

using namespace std;

int main()
{
  char firstName[] = "Richard";
  
  {
    char firstName[] = "Bob";
    cout << firstName << endl; 
  }
  
  cout << firstName << endl; 
  

   
   return 0;
}
